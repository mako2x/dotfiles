snippet f
  function(${1}) {
    ${2}
  };

snippet if
  if (${1}) {
    ${2}
  };

snippet ifel
  if (${1}) {
    ${2}
  } else {
  };

snippet for
	for (var ${2:i} = 0; $2 < ${1}.length; $2++) {
		${0}
	};

snippet key
  ${1}: ${2}


snippet timeout
  setTimeout(${2}, ${1});

snippet underscore
alias   _
  _(${1}).${2}

snippet jquery
alias   jq
  $('${1}')
